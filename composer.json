{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "^0.8",
        "beyondcode/laravel-websockets": "^1.0",
        "bugsnag/bugsnag-laravel": "^2.0",
        "doctrine/dbal": "^2.8",
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.7.1",
        "fideloper/proxy": "^4.0",
        "geekcom/validator-docs": "1.*",
        "guzzlehttp/guzzle": "^6.0",
        "intervention/image": "^2.4",
        "jenssegers/date": "^3.4",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^1.4",
        "laravel/tinker": "^1.0",
        "owen-it/laravel-auditing": "^8.0",
        "pragmarx/coollection": "^0.6.1",
        "pragmarx/firewall": "^2.2",
        "pragmarx/health": "^0.9",
        "pragmarx/sqli": "dev-master",
        "pragmarx/version": "^0.2.8",
        "pragmarx/zipcode": "^1.5",
        "predis/predis": "^1.1",
        "rollbar/rollbar-laravel": "^4.0",
        "sensiolabs/security-checker": "^5.0",
        "villfa/invert-color": "^0.4.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "beyondcode/dusk-dashboard": "^1.1",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.6",
        "laravel/telescope": "^0.1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "rollbar/rollbar-laravel"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
